mutate(
indicator_5A = case_when(
spot5A >= lagspot5A ~  1,
spot5A < lagspot5A ~ 0
)
)
## Create indicators using third industry development level
tlaged_ct_spdata_long <- tlaged_ct_spdata_long %>%
mutate(
indicator_terti = case_when(
terti >= lagterti ~  1,
terti < lagterti ~ 0
)
)
summary(tlaged_ct_spdata_long)
